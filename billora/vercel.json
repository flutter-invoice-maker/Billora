{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web",
        "buildCommand": "chmod +x build.sh && ./build.sh",
        "installCommand": "echo 'Flutter dependencies will be installed by build script'"
      }
    }
  ],
  "routes": [
    {
      "src": "/flutter_bootstrap.js",
      "dest": "/flutter_bootstrap.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/main.dart.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/canvaskit/(.*)",
      "dest": "/canvaskit/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 