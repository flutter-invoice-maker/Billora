{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/flutter_bootstrap.js",
      "dest": "/flutter_bootstrap.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/main.dart.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/canvaskit/(.*)",
      "dest": "/canvaskit/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 